{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMq": {
    "HostName": "host.docker.internal"
  },
  "EventBus": {
    "QueueName": "basket-microservice"
  },
  "Redis": {
    "Configuration": "host.docker.internal:6379"
